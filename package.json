{
  "name": "ecopolyana",
  "version": "1.0.0",
  "private": true,
  "description": "Платформа умной охоты Экополяна с конвертером данных и интеграцией Dadata",
  "author": "Olegadzhan",
  "license": "MIT",
  "homepage": "https://ecopolyana.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Olegadzhan/ecopolyana.git"
  },
  "bugs": {
    "url": "https://github.com/Olegadzhan/ecopolyana/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "vercel-build": "next build",
    "analyze": "ANALYZE=true next build",
    "postinstall": "npm install critters --save-dev || true"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "critters": "^0.0.20",
    "date-fns": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "framer-motion": "^11.3.7",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "papaparse": "^5.4.1",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-leaflet": "^4.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/leaflet": "^1.9.12",
    "@types/papaparse": "^5.3.14",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": ["eslint --fix", "prettier --write"],
    "src/**/*.{js,jsx,json,md}": ["prettier --write"]
  },
  "browserslist": ["defaults", "not IE 11"]
}
